    "react": "^16.4.1",
    "react-bootstrap-sweetalert": "^4.4.1",
    "react-chartist": "0.13.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.4.1",
    "react-loading": "^2.0.2",
